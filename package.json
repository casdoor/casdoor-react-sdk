{
  "name": "casdoor-react-sdk",
  "version": "0.0.1",
  "description": "React client SDK for Casdoor",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint:all": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "maxConcurrency": 1,
    "maxWorkers": 1,
    "testTimeout": 30000,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/casdoor/casdoor-react-sdk.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "auth",
    "authentication",
    "sso",
    "oauth",
    "oidc",
    "casbin",
    "casdoor"
  ],
  "author": {
    "name": "wenxuan70",
    "email": "t736660416@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/casdoor/casdoor-react-sdk/issues"
  },
  "homepage": "https://github.com/casdoor/casdoor-react-sdk#readme",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "casdoor-js-sdk": "^0.3.0",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  }
}
